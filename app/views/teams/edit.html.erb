<div class="container">
  <div class="col-md-4 col-md-offset-4">
    <h1 class='roboto'>Edit Team</h1>
    <%= simple_form_for @team do |f| %>
      <%= f.input :name %>
      <%= f.input_field :division_id, collection: @divisions %>
      <%= f.submit "apply changes", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
